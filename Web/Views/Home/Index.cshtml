@model TenantInfo

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    @if (Model != null)
    {
        <div>
            <strong>Tenant ID:</strong> @Model.TenantId
        </div>
        <div>
            <strong>Tenant Name:</strong> @Model.TenantName
        </div>
        <div>
            <strong>User ID:</strong> @Model.UserId
        </div>
    }
    else
    {
        <p>No Tenant/User info found.</p>
    }
</div>
